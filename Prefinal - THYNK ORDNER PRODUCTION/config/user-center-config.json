{
  "user_center": {
    "routes": {
      "/userCenter": {
        "description": "User Center Hauptseite",
        "permissions": ["authenticated_user"],
        "components": [
          "dashboard",
          "profile",
          "settings",
          "orders",
          "analytics"
        ]
      },
      "/sign-in": {
        "description": "Anmeldung",
        "permissions": ["public"]
      }
    },
    "user_roles": {
      "admin": {
        "permissions": [
          "view_all_orders",
          "edit_all_orders",
          "manage_users",
          "configure_system",
          "view_analytics",
          "export_data"
        ]
      },
      "manager": {
        "permissions": [
          "view_team_orders",
          "edit_team_orders",
          "view_analytics",
          "export_data"
        ]
      },
      "user": {
        "permissions": [
          "view_own_orders",
          "create_orders",
          "edit_own_orders"
        ]
      }
    },
    "dashboard_config": {
      "widgets": [
        "order_summary",
        "revenue_chart",
        "pending_orders",
        "recent_activity",
        "quick_stats"
      ],
      "refresh_interval": 30,
      "default_view": "overview"
    },
    "preferences": {
      "language": "de",
      "timezone": "Europe/Berlin",
      "date_format": "DD.MM.YYYY",
      "currency": "EUR",
      "notifications": {
        "email": true,
        "push": false,
        "sms": false
      }
    }
  },
  "note": "User Center Konfiguration aus thynkorders.com - muss 1:1 Ã¼bernommen werden",
  "source": "https://thynkorders.com/#/userCenter"
}

